{
    "name": "yojitu/seminar-system",
    "description": "セミナー申込・決済・管理システム",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "square/square": "^35.0",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Seminar\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
